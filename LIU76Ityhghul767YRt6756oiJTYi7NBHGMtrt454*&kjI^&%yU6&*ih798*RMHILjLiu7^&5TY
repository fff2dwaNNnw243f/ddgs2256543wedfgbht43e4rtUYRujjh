        if value == true then 
            GUI:Notify({
                Title = "Note",
                Content = "AntiBnag is Active!",
                Duration = 5.2,
            })
            antibang()
        else 
            GUI:Notify({
                Title = "Note",
                Content = "AntiBang Disabled..",
                Duration = 5.2,
            })
            unantibang()
        end
    end)

